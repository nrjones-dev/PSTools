{
  "manifestVersion": 5,
  "id": "rename.layers.plugin",
  "name": "Rename Layers",
  "version": "1.0.0",
  "main": "index.html",
  "author": "Nathan Jones",
  "description": "A plugin to rename layers based on user input.",
  "host": {
    "app": "PS",
    "minVersion": "23.3.0",
    "data": {
      "apiVersion": 2,
      "loadEvent": "use"
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "layercreation",
      "label": {
        "default": "Layer Renamer"
      },
      "minimumSize": {
        "width": 300,
        "height": 300
      },
      "maximumSize": {
        "width": 600,
        "height": 500
      },
      "preferredDockedSize": {
        "width": 300,
        "height": 350
      },
      "preferredFloatingSize": {
        "width": 450,
        "height": 400
      }
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "request"
  }
}
