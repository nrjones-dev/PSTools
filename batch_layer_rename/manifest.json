{
  "manifestVersion": 5,
  "id": "nrj.layer-utility",
  "name": "Layer Utilities",
  "version": "1.0.0",
  "main": "index.html",
  "author": "Nathan Jones",
  "description": "A plugin to rename layers, add a suffix and prefix and delete empty layers.",
  "host": {
    "app": "PS",
    "minVersion": "23.3.0",
    "data": {
      "apiVersion": 2,
      "loadEvent": "use"
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "nrj.layer-utility",
      "label": {
        "default": "Layer Utilities"
      },
      "minimumSize": {
        "width": 450,
        "height": 300
      },
      "maximumSize": {
        "width": 600,
        "height": 1000
      },
      "preferredDockedSize": {
        "width": 450,
        "height": 500
      },
      "preferredFloatingSize": {
        "width": 450,
        "height": 600
      }
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "request"
  },
  "featureFlags": {
    "enableAlerts": true
  }
}
